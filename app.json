{
  "expo": {
    "name": "In_Ur_Space",
    "slug": "In_Ur_Space",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SplashScreen/logo.png",
    "scheme": "inurspace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SplashScreen/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.manzar0911.In_Ur_Space"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SplashScreen/logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6c6dc486-3d7f-4d38-b92d-17306c4b8aa4"
      }
    }
  }
}
